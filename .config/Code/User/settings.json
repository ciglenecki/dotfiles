{
	"workbench.iconTheme": "material-icon-theme",
	"editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.fontLigatures": true,
	"workbench.editor.highlightModifiedTabs": true, // Don't lose your working by turning on autosave,
	"explorer.sortOrder": "type",
	"files.trimFinalNewlines": true,
	"editor.mouseWheelZoom": true,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
	},
	"breadcrumbs.icons": false,
	"breadcrumbs.showArrays": false,
	"breadcrumbs.showBooleans": false,
	"breadcrumbs.showConstants": false,
	"breadcrumbs.showConstructors": false,
	"breadcrumbs.showEnumMembers": false,
	"breadcrumbs.showEnums": false,
	"breadcrumbs.showEvents": false,
	"breadcrumbs.showFields": false,
	"breadcrumbs.showFiles": false,
	"breadcrumbs.showFunctions": false,
	"breadcrumbs.showKeys": false,
	"breadcrumbs.showInterfaces": false,
	"breadcrumbs.showMethods": false,
	"breadcrumbs.showModules": false,
	"breadcrumbs.showNamespaces": false,
	"breadcrumbs.showNull": false,
	"breadcrumbs.showNumbers": false,
	"breadcrumbs.showObjects": false,
	"breadcrumbs.showOperators": false,
	"breadcrumbs.showPackages": false,
	"breadcrumbs.showProperties": false,
	"breadcrumbs.showStrings": false,
	"breadcrumbs.showStructs": false,
	"breadcrumbs.showTypeParameters": false,
	"breadcrumbs.showVariables": false,
	"breadcrumbs.showClasses": false,
	"window.menuBarVisibility": "compact",
	"editor.minimap.renderCharacters": false,
	"editor.minimap.showSlider": "always",
	"workbench.colorCustomizations": {
		// Bracket Pair Colorizer overrides.
		"colorizer.color-1": "#ff9900",
		"colorizer.color-2": "#f500f5",
		"colorizer.color-3": "#54b9f8"
	},
	"bracket-pair-colorizer-2.colors": [
		"colorizer.color-1",
		"colorizer.color-2",
		"colorizer.color-3"
	],
	"javascript.updateImportsOnFileMove.enabled": "always",
	"todo-tree.tree.showScanModeButton": false,
	"bracketPairColorizer.showBracketsInGutter": true,
	"bracketPairColorizer.showBracketsInRuler": true,
	"BracketHighlighter.blurOpacity": "0.8",
	"BracketHighlighter.highlightScopeFromText": true,
	"BracketHighlighter.timeOutValue": 30,
	"BracketHighlighter.backgroundColor": "Crimson",
	"BracketHighlighter.textColor": "white",
	"editor.minimap.maxColumn": 60,
	"gitlens.blame.highlight.locations": [],
	"typescript.updateImportsOnFileMove.enabled": "always",
	"editor.cursorWidth": 2,
	"editor.cursorStyle": "block",
	"[markdown]": {
		"editor.quickSuggestions": true,
		"editor.formatOnSave": false,
		"files.trimTrailingWhitespace": false,
		"editor.defaultFormatter": "yzhang.markdown-all-in-one",
	},
	"editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
	"vim.insertModeKeyBindings": [
		{
			"before": [
				"<C-b>"
			],
			"after": []
		}
	],
	"spellright.language": [
		"en_US",
		"hr_HR"
	],
	"cSpell.userWords": [
		"funckija"
	],
	"editor.multiCursorMergeOverlapping": false,
	"markdown.extension.list.indentationSize": "inherit",
	"workbench.startupEditor": "newUntitledFile",
	"typescript.tsserver.trace": "verbose",
	"window.title": "${folderPath} ${activeEditorLong} ",
	"editor.snippetSuggestions": "top",
	"emmet.showSuggestionsAsSnippets": true,
	"explorer.confirmDelete": false,
	"movets.relativeToTsconfig": true,
	"git.autofetch": true,
	"editor.insertSpaces": false,
	"python.pythonPath": "/usr/bin/python3",
	"prettier.useEditorConfig": false,
	"prettier.printWidth": 120,
	"stories.username": "North Korea_global",
	"[cpp]": {
		"editor.defaultFormatter": "ms-vscode.cpptools"
	},
	"C_Cpp.vcFormat.space.betweenEmptyLambdaBrackets": true,
	"C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0 }",
	"editor.wordWrap": "on",
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"gitlens.views.searchAndCompare.files.layout": "tree",
	"gitlens.views.commits.files.layout": "tree",
	"diffEditor.codeLens": true,
	"typescript.implementationsCodeLens.enabled": true,
	"editor.rename.enablePreview": false,
	"typescript.preferences.importModuleSpecifier": "relative",
	"markdown.preview.openMarkdownLinks": "inEditor",
	"pasteImage.path": "${projectRoot}/pic",
	"terminal.integrated.cursorBlinking": true,
	"anki.md.card.separator": "::: q",
	"anki.md.card.frontBackSeparator": "\\n",
	"window.restoreWindows": "none",
	"vim.useSystemClipboard": true,
	"vim.normalModeKeyBindingsNonRecursive": [],
	"vim.handleKeys": {
		"<C-k>": false,
		"<C-d>": true
	},
	"markdown.links.openLocation": "beside",
	"markdown.styles": [
		"./.vscode/style.css"
	],
	"spellright.documentTypes": [
		"markdown",
		"latex",
		"plaintext"
	],
	"markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
	"markdown-preview-enhanced.codeBlockTheme": "github.css",
	"files.associations": {
		"*.txt": "plaintext"
	},
	"gitlens.blame.highlight.enabled": false,
	"gitlens.blame.heatmap.enabled": false,
	"gitlens.codeLens.enabled": false,
	"typescript.preferences.quoteStyle": "double",
	"prettier.singleQuote": true,
	"prettier.trailingComma": "all",
	"prettier.ignorePath": "",
	"window.openFilesInNewWindow": "default",
	"window.openFoldersInNewWindow": "off",
	"workbench.editor.enablePreviewFromQuickOpen": true,
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"window.closeWhenEmpty": true,
	"editor.wordWrapColumn": 120,
	"[c]": {
		"editor.defaultFormatter": "ms-vscode.cpptools"
	},
	"diffEditor.renderSideBySide": false,
	"editor.formatOnSave": true,
	"debug.javascript.autoAttachFilter": "smart",
	"java.dependency.showMembers": true,
	"terminal.integrated.tabs.enabled": true,
	"security.workspace.trust.enabled": false,
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"explorer.confirmDragAndDrop": false,
	"workbench.colorTheme": "Quiet Light",
	"python.defaultInterpreterPath": "/usr/bin/python3",
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
	},
	"python.formatting.provider": "black",
	"[yaml]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
}