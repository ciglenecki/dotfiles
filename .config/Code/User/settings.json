{
  "workbench.panel.defaultLocation": "right",
  "editor.fontFamily": "'Noto Mono', 'IntelOne Mono', 'Hack'",
  "editor.fontLigatures": true,
  "workbench.editor.highlightModifiedTabs": true, // Don't lose your working by turning on autosave,
  "explorer.sortOrder": "type",
  "files.trimFinalNewlines": true,
  "editor.mouseWheelZoom": true,
  "breadcrumbs.icons": false,
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showConstructors": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showEnums": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showFiles": false,
  "breadcrumbs.showFunctions": false,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showInterfaces": false,
  "breadcrumbs.showMethods": false,
  "breadcrumbs.showModules": false,
  "breadcrumbs.showNamespaces": false,
  "breadcrumbs.showNull": false,
  "breadcrumbs.showNumbers": false,
  "breadcrumbs.showObjects": false,
  "breadcrumbs.showOperators": false,
  "breadcrumbs.showPackages": false,
  "breadcrumbs.showProperties": false,
  "breadcrumbs.showStrings": false,
  "breadcrumbs.showStructs": false,
  "breadcrumbs.showTypeParameters": false,
  "breadcrumbs.showVariables": false,
  "breadcrumbs.showClasses": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "workbench.colorCustomizations": {
    "colorizer.color-1": "#ff9900",
    "colorizer.color-2": "#f500f5",
    "colorizer.color-3": "#54b9f8"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.maxColumn": 60,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.cursorWidth": 2,
  "editor.cursorStyle": "block",
  "[markdown]": {
    // "editor.quickSuggestions": true,
    "editor.formatOnSave": false,
    "files.trimTrailingWhitespace": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "<C-b>"
      ],
      "after": []
    }
  ],
  "spellright.language": [
    "en_US",
    "hr_HR"
  ],
  "cSpell.userWords": [
    "Allreduce",
    "Blokirajuće",
    "Brentovo",
    "Convolutional",
    "Defaultni",
    "downsweap",
    "ERCW",
    "funckija",
    "Guessr",
    "interpretability",
    "Krupnozrnatost",
    "međuvrijednosti",
    "Neblokirajući",
    "nelokalnog",
    "paralelizirati",
    "podniz",
    "podnizove",
    "prescan",
    "prescana",
    "psuma",
    "settamo",
    "sweap"
  ],
  "editor.multiCursorMergeOverlapping": false,
  "workbench.startupEditor": "newUntitledFile",
  "typescript.tsserver.trace": "verbose",
  "window.title": "${folderPath} ${activeEditorLong} ",
  "editor.snippetSuggestions": "top",
  "emmet.showSuggestionsAsSnippets": true,
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "C_Cpp.vcFormat.space.betweenEmptyLambdaBrackets": true,
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0 }",
  "editor.wordWrap": "on",
  "gitlens.views.searchAndCompare.files.layout": "tree",
  "gitlens.views.commits.files.layout": "tree",
  "diffEditor.codeLens": true,
  "typescript.implementationsCodeLens.enabled": true,
  "editor.rename.enablePreview": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "markdown.preview.openMarkdownLinks": "inEditor",
  "pasteImage.path": "${projectRoot}/pic",
  "terminal.integrated.cursorBlinking": true,
  "window.restoreWindows": "none",
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindingsNonRecursive": [],
  "vim.handleKeys": {
    "<C-k>": false,
    "<C-d>": true,
    "<C-f>": false,
    "<C-b>": false,
    "<C-p>": false,
  },
  "markdown.links.openLocation": "beside",
  "spellright.documentTypes": [
    "latex",
    "plaintext",
    "markdown"
  ],
  "markdown-preview-enhanced.codeBlockTheme": "github.css",
  "gitlens.blame.highlight.enabled": false,
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.codeLens.enabled": false,
  "typescript.preferences.quoteStyle": "double",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.ignorePath": "",
  "window.openFoldersInNewWindow": "on",
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "window.closeWhenEmpty": true,
  "editor.wordWrapColumn": 120,
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "debug.javascript.autoAttachFilter": "disabled",
  "java.dependency.showMembers": true,
  "terminal.integrated.tabs.enabled": true,
  "security.workspace.trust.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "explorer.confirmDragAndDrop": false,
  "settingsSync.ignoredExtensions": [
    "ctf0.laravel-goto-env"
  ],
  "debug.onTaskErrors": "showErrors",
  "workbench.editorAssociations": {
    "*.mat": "default",
    "*.class": "default"
  },
  "jupyter.askForKernelRestart": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "http.systemCertificates": false,
  "markdownlint.config": {
    "MD040": false, // fenced code block allow no language spec
    "MD033": false, // allow inline html
    "MD045": false, // no image text
    "MD025": true,
    "MD010": false,
    "MD034": false
  },
  "markdown-preview-enhanced.previewTheme": "github-light.css",
  "terminal.integrated.inheritEnv": false,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "autoDocstring.customTemplatePath": "autoDocStringTemplate.mustache",
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
  },
  "isort.args": [
    "--profile",
    "black"
  ],
  "notebook.output.textLineLimit": 300,
  "scm.diffDecorations": "none",
  "python.analysis.exclude": [
    "**/venv",
    "**/__pycache__",
    "**/.pytest_cache",
    "**/.mypy_cache",
    "**/data",
    "**/embeddings",
    "**/output",
    "data/",
    "embeddings/",
    "output/",
    "**/basicsr"
  ],
  "editor.scrollbar.verticalScrollbarSize": 10,
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": "true",
    "scminput": false,
    "cpp": false
  },
  "terminal.integrated.scrollback": 999999,
  "workbench.colorTheme": "Quiet Light",
  "editor.trimAutoWhitespace": false,
  "python.experiments.enabled": false,
  "python.analysis.diagnosticMode": "workspace",
  "workbench.editor.empty.hint": "hidden",
  "cmake.configureOnOpen": true,
  "files.autoSave": "afterDelay",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.venvPath": "${workspaceFolder}/venv",
  "ruff.lint.run": "onSave",
  "python.analysis.logLevel": "Warning",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoFormatStrings": true,
  "python.analysis.fixAll": [
    "source.convertImportFormat"
  ],
  "python.defaultInterpreterPath": "/home/matej/venv",
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "basic",
  "github.copilot.editor.enableAutoCompletions": true,
  "editor.formatOnSave": true,
  "ruff.lint.args": [
    "[\"--unfixable F401\"]"
  ],
  "remote.SSH.serverInstallPath": {
    "tp-komp-ured": "/home/matejc/"
  },
  "remote.autoForwardPortsSource": "hybrid",
  "diffEditor.maxComputationTime": 0,
  "terminal.integrated.cwd": "${workspaceFolder}",
}